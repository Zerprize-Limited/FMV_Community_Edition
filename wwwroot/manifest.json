{
  "name": "FRAM Model Visualiser",
  "short_name": "FMV",
  "start_url": "./",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#03173d",
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "FRAM-512.png",
      "type": "image/png",
      "sizes": "512x512"
    },
    {
      "src": "FRAM-192.png",
      "type": "image/png",
      "sizes": "192x192"
    }
  ],
  "file_handlers": [
    {
      "action": "./",
      "accept": {
        "application/xml": [ ".xfmv" ]
      }
    }
  ]
}
